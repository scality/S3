{
  "name": "s3",
  "version": "1.1.0",
  "description": "S3 connector",
  "main": "index.js",
  "engines": {
    "node": "~6.9.1"
  },
  "repository": "scality/S3",
  "keywords": [
    "s3",
    "cloud",
    "server"
  ],
  "author": "Giorgio Regni",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/scality/S3/issues"
  },
  "homepage": "https://github.com/scality/S3#readme",
  "dependencies": {
    "arsenal": "scality/Arsenal#ft/node-v6",
    "async": "1.4.2",
    "babel-core": "6.18.2",
    "babel-plugin-array-includes": "2.0.3",
    "babel-plugin-transform-es2015-destructuring": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-es2015-parameters": "6.18.0",
    "bucketclient": "scality/bucketclient#ft/node-v6",
    "commander": "2.9.0",
    "level": "1.5.0",
    "level-sublevel": "6.6.1",
    "multilevel": "7.3.0",
    "node-uuid": "1.4.7",
    "ready-set-stream": "1.0.7",
    "sproxydclient": "scality/sproxydclient#ft/node-v6",
    "utapi": "scality/utapi#ft/node-v6",
    "utf8": "2.1.2",
    "vaultclient": "scality/vaultclient#ft/node-v6",
    "werelogs": "scality/werelogs#ft/node-v6",
    "xml2js": "0.4.17"
  },
  "optionalDependencies": {
    "ioctl": "2.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-eslint": "6.1.2",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "4.3.0",
    "eslint-config-scality": "scality/Guidelines#ft/node-v6",
    "istanbul": "1.0.0-alpha.2",
    "istanbul-api": "1.0.0-alpha.13",
    "lolex": "1.5.1",
    "mocha": "2.5.3",
    "mocha-junit-reporter": "1.11.1",
    "node-mocks-http": "1.5.4",
    "s3blaster": "scality/s3blaster#ft/node-v6"
  },
  "scripts": {
    "ft_test": "mocha tester.js --compilers js:babel-core/register",
    "init": "node init.js",
    "lint": "eslint $(git ls-files '*.js')",
    "lint_md": "mdlint $(git ls-files '*.md')",
    "mem_backend": "S3BACKEND=mem node index.js",
    "perf": "mocha --compilers js:babel-core/register tests/performance/s3standard.js",
    "start": "node init.js && node index.js",
    "start_utapi": "node utapiServer.js",
    "test": "S3BACKEND=mem mocha --compilers js:babel-core/register --recursive tests/unit",
    "unit_coverage": "mkdir -p coverage/unit/ && S3BACKEND=mem MOCHA_FILE=$CIRCLE_TEST_REPORTS/unit/unit.xml istanbul cover --dir coverage/unit _mocha -- --compilers js:babel-core/register --reporter mocha-junit-reporter --recursive tests/unit"
  }
}
